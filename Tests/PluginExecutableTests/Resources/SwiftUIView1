enum SomeEnum { }

actor SomeActor { }

extension View {
    func borderedCaption() -> some View {
        modifier(BorderedCaption())
    }
}

struct SwiftUIView: View {
    var body: some View {
        Text("Hello, World!")
            .padding()
    }
}

